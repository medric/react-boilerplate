    },
    "betterScripts": {
        "build:dev": {
            "command": "webpack --config ./webpack/webpack.config.js --progress --colors",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "build:dist": {
            "command": "webpack --config ./webpack/webpack.config.js --progress --colors",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "test": {
            "command": "karma start",
            "env": {
                "NODE_ENV": "test"
            }
        }
    },
