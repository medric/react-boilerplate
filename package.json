  "version": "1.0.0",
  "description": "",
  "main": "./src/server/index.js",
  "scripts": {
    "build:dev": "better-npm-run build:dev",
    "start": "npm run build:dev && ./node_modules/.bin/babel-node ./server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack --config ./webpack/webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:dist": {
      "command": "webpack --config ./webpack/webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
